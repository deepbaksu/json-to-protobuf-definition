{
  "name": "json-to-protobuf-definition",
  "version": "1.0.0",
  "description": "Generate Protocol Buffer definition from JSON",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "repository": "https://github.com/kkweon/json-to-protobuf-definition.git",
  "author": "Mo Kweon <kkweon@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublish": "yarn clean && yarn build",
    "build": "yarn protoc && tsc",
    "test": "jest",
    "format": "prettier \"*.{js,ts,json,yml,yaml,md}\" \"src/**/*.{ts,js,json}\" --write",
    "clean": "rm -rf dist",
    "protoc": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:. --ts_out=. src/**/*.proto"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/lodash.snakecase": "^4.1.6",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "lodash.snakecase": "^4.1.1"
  }
}
